{
    "presets": [
        "@babel/env",
        "@babel/typescript",
        "@babel/react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy":  true }],
        ["@babel/proposal-class-properties", { "loose":  true }],
        "@babel/proposal-object-rest-spread",
        "babel-plugin-styled-components",
    ]
}